{
  "name": "aslam/bank-bri",
  "description": "Laravel package for BRI API",
  "keywords": [
    "laravel-bri",
    "bri-api",
    "laravel-bank-bri"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aslam",
      "email": "iupin5212@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Aslam\\Bri\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Aslam\\Bri\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aslam\\Bri\\Providers\\BriServiceProvider"
      ],
      "aliases": {
        "BriAPI": "Aslam\\Bri\\Facades\\BriFacade"
      }
    }
  },
  "require": {
    "php": ">=7.2|^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~7.0",
    "illuminate/collections": "~7.0|~8.0",
    "illuminate/macroable": "~7.0|~8.0",
    "illuminate/support": "~7.0|~8.0"
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
